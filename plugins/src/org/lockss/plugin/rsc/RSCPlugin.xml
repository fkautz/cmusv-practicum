  <entry>
    <string>plugin_name</string>
    <string>Royal Society of Chemistry Plugin</string>
  </entry>
  <entry>
    <string>au_def_new_content_crawl</string>
    <long>1209600000</long>
  </entry>
  <entry>
    <string>plugin_notes</string>
    <string></string>
  </entry>
  <entry>
    <string>au_crawlrules</string>
    <list>
      <string>1,&quot;%s.*&quot;, resolver_url</string>
      <string>4,&quot;^%s&quot;, base_url</string>
      <string>1,&quot;^%spublishing/journals/lockss/\?journalid=%s&amp;volume=%d.*&quot;, base_url, journal_id, volume</string>
      <string>1,&quot;^%spublishing/journals/lockss/\?journalid=%s.*&amp;year=%d.*&quot;, base_url, journal_id, year</string>
      <string>1,&quot;^%s.*\.(js|css|ico|gif|jpg|jpeg|png)&quot;, base_url</string>
      <string>1,&quot;%spublishing/journals/.*article\.asp\?doi=.*&quot;, base_url</string>
      <string>1,&quot;%sej/.*\.pdf&quot;, base_url</string>
      <string>1,&quot;%sdelivery/_ArticleLinking/DisplayArticleForFree\.cfm.*&quot;, base_url</string>
    </list>
  </entry>
  <entry>
    <string>au_start_url</string>
    <string>&quot;%spublishing/journals/lockss/?journalid=%s&quot;, base_url, journal_id</string>
  </entry>
