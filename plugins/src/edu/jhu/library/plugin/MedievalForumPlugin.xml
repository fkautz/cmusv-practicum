<?xml version="1.0" encoding="UTF-8"?>
<Elements>
<MapElement>
<ElementKey>plugin_config_props</ElementKey>
<ElementValue xsi:type="java:java.util.ArrayList" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
<ConfigParamDescr>
<Key>base_url</Key>
<DisplayName>Base URL</DisplayName>
<Description>Usually of the form http://&lt;journal-name&gt;.com/</Description>
<Type>3</Type>
<Size>40</Size>
<Definitional>true</Definitional>
</ConfigParamDescr>
<ConfigParamDescr>
<Key>volume</Key>
<DisplayName>Volume No.</DisplayName>
<Type>6</Type>
<Size>8</Size>
<Definitional>true</Definitional>
</ConfigParamDescr>
</ElementValue>
</MapElement>
<MapElement>
<ElementKey>au_crawlrules</ElementKey>
<ElementValue xsi:type="java:java.util.ArrayList" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
<string xsi:type="java:java.lang.String">4,"^%s", base_url</string>
<string xsi:type="java:java.lang.String">1,"%sMedievalForum-volume%d.html", base_url, volume</string>
<string xsi:type="java:java.lang.String">1,"^%svolume%d\.html", base_url, volume</string>
<string xsi:type="java:java.lang.String">1,"^%s(Comments|Contact|Contactus|Guidelines|index)\.html", base_url</string>
<string xsi:type="java:java.lang.String">1,"^%sImages/.*", base_url</string>
<string xsi:type="java:java.lang.String">1,"^%s[Vv]olume(%%20)?%d/.*", base_url, volume</string>
</ElementValue>
</MapElement>
<MapElement>
<ElementKey>au_start_url</ElementKey>
<ElementValue xsi:type="java:java.lang.String" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">"%sMedievalForum-volume%d.html", base_url, volume</ElementValue>
</MapElement>
<MapElement>
<ElementKey>plugin_name</ElementKey>
<ElementValue xsi:type="java:java.lang.String" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">Medieval Forum</ElementValue>
</MapElement>
<MapElement>
<ElementKey>au_name</ElementKey>
<ElementValue xsi:type="java:java.lang.String" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">"Medieval Forum Volume %d", volume</ElementValue>
</MapElement>
<MapElement>
<ElementKey>plugin_identifier</ElementKey>
<ElementValue xsi:type="java:java.lang.String" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">edu.jhu.library.plugin.MedievalForumPlugin</ElementValue>
</MapElement>
</Elements>
