{

  publisher <
    name = Alabama Department of Archives and History
  >

  {

    plugin = gov.alabama.archives.adpn.executiveorders.ExecutiveOrdersPlugin
    param[base_url] = http://www.archives.alabama.gov/lockss/    
    implicit < status ; name >
  
    {

      title <
        name = Alabama Department of Archives and History Executive Orders 
      >

      au < down ; Alabama Department of Archives and History Executive Orders >
  
    }
  
  }
  
  {

    plugin = gov.alabama.archives.adpn.goldstar.GoldstarPlugin
    param[base_url] = http://www.archives.alabama.gov/lockss/
    implicit < status ; name ; param[subdirectory] >
  
    {

      title <
        name = Alabama Department of Archives and History World War I Gold Star Database 
      >

      au < down ; Alabama Department of Archives and History World War I Gold Star Database 1 ; goldstar1 >
      au < released ; Alabama Department of Archives and History World War I Gold Star Database 2 ; goldstar2 >
  
    }
  
  }

}