{

  publisher <
    name = Berkeley Electronic Press
  >
  
  plugin = org.lockss.plugin.bepress.ClockssBerkeleyElectronicPressPlugin
  param[base_url] = http://www.bepress.com/
  implicit < status ; year ; name ; param[volume] >
  
  {

    title <
      name = Asian Journal of Comparative Law
    >

    param[journal_abbr] = asjcl

    au < manifest ; 2008 ; Asian Journal of Comparative Law Volume 3 ; 3 >
