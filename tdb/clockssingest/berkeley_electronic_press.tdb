    au < manifest ; 2009 ; The B.E. Journal of Economic Analysis & Policy Volume 9 ; 9 >

  }

  {

    title <
      name = The B.E. Journal of Macroeconomics
    >

    param[journal_abbr] = bejm

    au < manifest ; 2008 ; The B.E. Journal of Macroeconomics Volume 8 ; 8 >
    au < manifest ; 2009 ; The B.E. Journal of Macroeconomics Volume 9 ; 9 >

  }

  {

    title <
      name = The B.E. Journal of Theoretical Economics
    >

    param[journal_abbr] = bejte

    au < manifest ; 2008 ; The B.E. Journal of Theoretical Economics Volume 8 ; 8 >
    au < manifest ; 2009 ; The B.E. Journal of Theoretical Economics Volume 9 ; 9 >

  }

  {

    title <
      name = Basic Income Studies
    >

