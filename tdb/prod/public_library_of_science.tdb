{
  publisher <
    name = Public Library of Science
  >
  
  rights = openaccess
  plugin = org.lockss.plugin.topaz.TopazPlugin
  implicit < status ; year ; name ; param[year] >

  {
    title <
      name = PLoS Genetics ;
      issn = 1553-7390 ;
      eissn = 1553-7404
    >
    
    param[base_url] = http://www.plosgenetics.org/    
    param[journal_id] = genetics

    au < released ; 2005 ; PLoS Genetics Volume 1 ; 2005 >
    au < released ; 2006 ; PLoS Genetics Volume 2 ; 2006 >
    au < released ; 2007 ; PLoS Genetics Volume 3 ; 2007 >
    au < released ; 2008 ; PLoS Genetics Volume 4 ; 2008 >

  }

  {
    title <
      name = PLoS Computational Biology ;
      issn = 1553-734X ;
      eissn = 1553-7358
    >
    
    param[base_url] = http://www.ploscompbiol.org/    
    param[journal_id] = compbiol

    au < released ; 2005 ; PLoS Computational Biology Volume 1 ; 2005 >
    au < released ; 2006 ; PLoS Computational Biology Volume 2 ; 2006 >
    au < released ; 2007 ; PLoS Computational Biology Volume 3 ; 2007 >
    au < released ; 2008 ; PLoS Computational Biology Volume 4 ; 2008 >

  }


  {
    title <
      name = PLoS One ;
      issn = 1932-6203
    >
    
    comment[0] = repository error, too many files
    
    param[base_url] = http://www.plosone.org/    
    param[journal_id] = plosone

    au < notReady ; 2006 ; PLoS One Volume 1 ; 2006 >
    au < notReady ; 2007 ; PLoS One Volume 2 ; 2007 >
    au < notReady ; 2008 ; PLoS One Volume 3 ; 2008 >

  }
  
  
  {
    title <
      name = PLoS Neglected Tropical Diseases ;
      issn = 1935-2735
    >
    
    param[base_url] = http://www.plosntds.org/    
    param[journal_id] = ntd

    au < released ; 2007 ; PLoS Neglected Tropical Diseases Volume 1 ; 2007 >
    au < released ; 2008 ; PLoS Neglected Tropical Diseases Volume 2 ; 2008 >

  }
  
  
  {
    title <
      name = PLoS Pathogens ;
      issn = 1553-7366 ;
      eissn = 1553-7374
    >
    
    param[base_url] = http://www.plospathogens.org/    
    param[journal_id] = pathogens

    au < released ; 2005 ; PLoS Pathogens Volume 1 ; 2005 >
    au < released ; 2006 ; PLoS Pathogens Volume 2 ; 2006 >
    au < released ; 2007 ; PLoS Pathogens Volume 3 ; 2007 >
    au < released ; 2008 ; PLoS Pathogens Volume 4 ; 2008 >
    au < released ; 2009 ; PLoS Pathogens Volume 5 ; 2009 >

  }
  
  
  {
    title <
      name = PLoS Hub Clinical Trials
    >

    comment[plos0] = no manifest

    param[base_url] = http://clinicaltrials.ploshubs.org/    
    param[journal_id] = clinicalTrials

    au < exists ; 2006 ; PLoS Clinical Trials Volume 1 ; 2006 >
    au < exists ; 2007 ; PLoS Clinical Trials Volume 2 ; 2007 >
    au < exists ; 2008 ; PLoS Clinical Trials Volume 3 ; 2008 >

  }

  {
    title <
      name = PLoS Medicine ;
      issn = 1549-1277 ;
      eissn = 1549-1676
    >

    param[base_url] = http://www.plosmedicine.org/   
    param[journal_id] = medicine

    au < released ; 2004 ; PLoS Medicine Volume 1 ; 2004 >
    au < released ; 2005 ; PLoS Medicine Volume 2 ; 2005 >
    au < released ; 2006 ; PLoS Medicine Volume 3 ; 2006 >
    au < released ; 2007 ; PLoS Medicine Volume 4 ; 2007 >
    au < released ; 2008 ; PLoS Medicine Volume 5 ; 2008 >
    au < released ; 2009 ; PLoS Medicine Volume 6 ; 2009 >

  }

}
