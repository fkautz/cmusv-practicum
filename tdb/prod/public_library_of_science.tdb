    au < released ; 2006 ; PLoS Computational Biology Volume 2 ; 2006 >
    au < released ; 2007 ; PLoS Computational Biology Volume 3 ; 2007 >
    au < released ; 2008 ; PLoS Computational Biology Volume 4 ; 2008 >

  }


  {
    title <
      name = PLoS One ;
      issn = 1932-6203 ;
      issnl = 1932-6203
    >
    
    comment[0] = repository error, too many files
    
    param[base_url] = http://www.plosone.org/    
    param[journal_id] = plosone

    au < notReady ; 2006 ; PLoS One Volume 1 ; 2006 >
    au < notReady ; 2007 ; PLoS One Volume 2 ; 2007 >
    au < notReady ; 2008 ; PLoS One Volume 3 ; 2008 >

  }
  
  
  {
    title <
