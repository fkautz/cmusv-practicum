{
  publisher <
    name = Government Printing Office
  >

  rights = openaccess
  param[base_url] = http://bulk.resource.org/

  {
    title <
      name = Congressional Bills
    >

    implicit < status ; name ; param[congress] >
      
    plugin = org.lockss.plugin.usdocspln.gov.gpo.bills.CongressionalBillsPlugin

    au < manifest ; Congressional Bills, 104th Congress ; 103 >
    au < manifest ; Congressional Bills, 104th Congress ; 104 >
    au < manifest ; Congressional Bills, 105th Congress ; 105 >
    au < manifest ; Congressional Bills, 106th Congress ; 106 >
    au < manifest ; Congressional Bills, 107th Congress ; 107 >
    au < manifest ; Congressional Bills, 108th Congress ; 108 >
    au < manifest ; Congressional Bills, 109th Congress ; 109 >
    au < manifest ; Congressional Bills, 110th Congress ; 110 >

  }

  {
    title <
      name = Congressional Documents
    >

    implicit < status ; name ; param[congress] >
      
    plugin = org.lockss.plugin.usdocspln.gov.gpo.documents.CongressionalDocumentsPlugin

    au < manifest ; Congressional Documents, 104th Congress ; 104 >
    au < manifest ; Congressional Documents, 105th Congress ; 105 >
    au < manifest ; Congressional Documents, 106th Congress ; 106 >
    au < manifest ; Congressional Documents, 107th Congress ; 107 >
    au < manifest ; Congressional Documents, 108th Congress ; 108 >
    au < manifest ; Congressional Documents, 109th Congress ; 109 >
    au < manifest ; Congressional Documents, 110th Congress ; 110 >

  }

  {
    title <
      name = Congressional Hearings
    >

    implicit < status ; name ; param[congress] >
      
    plugin = org.lockss.plugin.usdocspln.gov.gpo.hearings.CongressionalHearingsPlugin

    au < exists ; Congressional Hearings, 104th Congress ; 104 >
    au < manifest ; Congressional Hearings, 105th Congress ; 105 >
    au < manifest ; Congressional Hearings, 106th Congress ; 106 >
    au < manifest ; Congressional Hearings, 107th Congress ; 107 >
    au < manifest ; Congressional Hearings, 108th Congress ; 108 >
    au < manifest ; Congressional Hearings, 109th Congress ; 109 >
    au < manifest ; Congressional Hearings, 110th Congress ; 110 >

  }

  {
    title <
      name = Constitution of the United States
    >

    implicit < status ; name ; param[year] >
      
    plugin = org.lockss.plugin.usdocspln.gov.gpo.constitution.AnnotatedConstitutionPlugin

    au < manifest ; Constitution of the United States, 1992 Edition ; 1992 >
    au < manifest ; Constitution of the United States, 2002 Edition ; 2002 >

  }

}
