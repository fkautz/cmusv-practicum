<?xml version="1.0" encoding="UTF-8"?>
<Elements>
<MapElement>
<ElementKey>plugin_config_props</ElementKey>
<ElementValue xsi:type="java:java.util.ArrayList" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
<ConfigParamDescr>
<Key>base_url</Key>
<DisplayName>Base URL</DisplayName>
<Description>Usually of the form http://&lt;journal-name&gt;.com/</Description>
<Type>3</Type>
<Size>40</Size>
<Definitional>true</Definitional>
</ConfigParamDescr>
<ConfigParamDescr>
<Key>year</Key>
<DisplayName>Year</DisplayName>
<Description>Four digit year (e.g., 2004)</Description>
<Type>4</Type>
<Size>4</Size>
<Definitional>true</Definitional>
</ConfigParamDescr>
</ElementValue>
</MapElement>
<MapElement>
<ElementKey>au_crawlrules</ElementKey>
<ElementValue xsi:type="java:java.util.ArrayList" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
<string xsi:type="java:java.lang.String">4,"^%s", base_url</string>
<string xsi:type="java:java.lang.String">1,"%scard/%d/", base_url, year</string>
<string xsi:type="java:java.lang.String">1,"%s%d/.*", base_url, year</string>
</ElementValue>
</MapElement>
<MapElement>
<ElementKey>au_start_url</ElementKey>
<ElementValue xsi:type="java:java.lang.String" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">"%scard/%d/", base_url, year</ElementValue>
</MapElement>
<MapElement>
<ElementKey>text/html_filter</ElementKey>
<ElementValue xsi:type="java:java.lang.String" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">org.lockss.plugin.locksscard.LockssCardFilterRule</ElementValue>
</MapElement>
<MapElement>
<ElementKey>plugin_name</ElementKey>
<ElementValue xsi:type="java:java.lang.String" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">LOCKSS Card</ElementValue>
</MapElement>
<MapElement>
<ElementKey>au_name</ElementKey>
<ElementValue xsi:type="java:java.lang.String" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">"LOCKSS Card %d", year</ElementValue>
</MapElement>
<MapElement>
<ElementKey>plugin_identifier</ElementKey>
<ElementValue xsi:type="java:java.lang.String" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">org.lockss.plugin.LockssCardPlugin</ElementValue>
</MapElement>
<MapElement>
<ElementKey>plugin_version</ElementKey>
<ElementValue xsi:type="java:java.lang.String" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">2</ElementValue>
</MapElement>
