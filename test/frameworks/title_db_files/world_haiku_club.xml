   </property>
   <property name="param.2">
    <property name="key" value="volume" />
    <property name="value" value="2" />
   </property>
   <property name="param.99">
    <property name="key" value="pub_down" />
    <property name="value" value="true" />
   </property>
   <property name="attributes.rights" value="openaccess" />
  </property>

  <property name="WorldHaikuReview3">
   <property name="attributes.publisher" value="World Haiku Club" />
   <property name="journalTitle" value="World Haiku Review" />
   <property name="title" value="World Haiku Review Volume 3" />
   <property name="plugin" value="edu.harvard.plugin.WorldHaikuReview.WorldHaikuReviewPlugin" />
   <property name="param.1">
    <property name="key" value="base_url" />
